{
  "name": "fauna-x-schemas",
  "type": "module",
  "version": "1.2.0",
  "description": "A collection of basic validation schemas to improve the typing of Fauna's new FQLx driver",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LiamMartens/fauna-x-schemas.git"
  },
  "homepage": "https://github.com/LiamMartens/fauna-x-schemas",
  "bugs": {
    "url": "https://github.com/LiamMartens/fauna-x-schemas/issues"
  },
  "author": "Liam Martens <hi@liammartens.com>",
  "license": "MIT",
  "peerDependencies": {
    "zod": "^3.0.0",
    "fauna": "^1.0.0"
  },
  "devDependencies": {
    "fauna": "^1.1.0",
    "typescript": "^5.2.2",
    "zod": "^3.22.2"
  }
}
